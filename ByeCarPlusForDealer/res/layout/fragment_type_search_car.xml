<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/typeSearchCarPage_mainLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5" >

    <com.byecar.byecarplusfordealer.views.TitleBar
        android:id="@+id/typeSearchCarPage_titleBar"
        android:layout_width="match_parent"
    	android:layout_height="0dp" />

    <ListView
        android:id="@+id/typeSearchCarPage_listView"
        android:layout_width="match_parent"
    	android:layout_height="match_parent"
    	android:layout_below="@id/typeSearchCarPage_titleBar"
    	android:visibility="invisible"
    	android:cacheColorHint="#0000" />
    
    <GridView
        android:id="@+id/typeSearchCarPage_gridView"
        android:layout_width="match_parent"
    	android:layout_height="match_parent"
    	android:layout_below="@id/typeSearchCarPage_titleBar"
    	android:visibility="invisible"
    	android:numColumns="4"
    	android:cacheColorHint="#0000" />
        
</RelativeLayout>
