<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
    <!-- 타이틀바. -->
    
    <com.cmons.cph.views.TitleBar
	    android:id="@+id/wholesaleSettingNoticePage_titleBar" 
	    android:layout_width="match_parent"
	    android:layout_height="0dp"
	    android:layout_alignParentLeft="true"
	    android:layout_alignParentTop="true" />

    <!-- 배경. -->
    
    <ImageView 
        android:id="@+id/wholesaleSettingNoticePage_ivBg" 
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:layout_below="@id/wholesaleSettingNoticePage_titleBar"
	    android:scaleType="centerCrop"
	    android:src="@drawable/setting_bg" />
    
    <ListView
        android:id="@+id/wholesaleSettingNoticePage_listView"
    	android:layout_width="match_parent"
    	android:layout_height="match_parent" />
    
</RelativeLayout>