<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/openablePostList_mainLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5" >

    <com.byecar.byecarplus.views.TitleBar
        android:id="@+id/openablePostList_titleBar"
        android:layout_width="match_parent"
    	android:layout_height="0dp" />

    <ListView 
        android:id="@+id/openablePostList_listView"
        android:layout_width="match_parent"
    	android:layout_height="match_parent"
    	android:layout_below="@id/openablePostList_titleBar"
    	android:cacheColorHint="#0000" />
        
</RelativeLayout>
